{
    "extends": "tslint:recommended",
    "rulesDirectory": [],
    "rules": {
        "arrow-parens": false,
        "interface-name": false,
        "member-access": [true, "check-accessor"],
        "no-consecutive-blank-lines": false,
        "object-literal-sort-keys": false,
        "prefer-const": [true, { "destructuring": "all" }],
        "quotemark": [true, "double"],
        "trailing-comma": [
            true,
            {
                "multiline": {
                    "objects": "never",
                    "arrays": "never",
                    "functions": "never"
                }
            }
        ]
    }
}
